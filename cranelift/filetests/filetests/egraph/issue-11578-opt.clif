test optimize
set opt_level=speed
target x86_64
target aarch64

function %eq_k1(i64) -> i8 {
block0(v2: i64):
    v14 = iconst.i64 -562949953421310
    v3 = band v2, v14
    v16 = iconst.i64 0
    v4 = icmp eq v3, v16
    v5 = iconst.i64 6
    v6 = iconst.i64 7
    v7 = select v4, v5, v6
    v8 = icmp eq v7, v5
    return v8
}

