; select x, k1, k1 should be constant propagated
function %constant_propagated(i64) -> i8 {
block0(v2: i64):
    v14 = iconst.i64 -562949953421310
    v3 = band v2, v14
    v16 = iconst.i64 0
    v4 = icmp eq v3, v16
    v5 = iconst.i64 6
    v6 = select v4, v5, v5
    v7 = icmp eq v6, v5
    return v7
}

