function %f2(i8) -> i8 {
block0(v1: i8):
    v2 = icmp eq v1, v1
    return v2
}

; check: v3 = iconst.i8 1
; check: return v3

function %f3(i8) -> i8 {
block0(v1: i8):
    v2 = icmp ne v1, v1
    return v2
}

; check: v3 = iconst.i8 0
; check: return v3

function %bnot1(i8) -> i8 {
block0(v1: i8):
    v2 = iconst.i8 -1
    v3 = bxor v1, v2
    return v3
}

; check: v4 = bnot v1
; check: return v4

