function %ult_via_cmp(i8, i8) -> i8 {
block0(v0: i8, v1: i8):
    v2 = icmp ugt v0, v1
    v3 = icmp ult v0, v1
    v4 = isub v2, v3
    v5 = iconst.i8 0
    v6 = icmp slt v4, v5
    return v6
    ; check: v3 = icmp ult v0, v1
    ; check: return v3
}

