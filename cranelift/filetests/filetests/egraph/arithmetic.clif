test optimize
set opt_level=speed
target x86_64

function %f0(i32) -> i32 {
block0(v0: i32):
    v1 = iconst.i32 4
    v2 = imul v0, v1
    ; check: v3 = iconst.i32 2
    ; check: v4 = ishl v0, v3  ; v3 = 2
    ; check: return v4
    return v2
}

function %double_ineg(i32) -> i32 {
block0(v0: i32):
    v1 = ineg v0
    v2 = ineg v1
    return v2
    ; check: return v0
}

function %add_ineg(i32, i32) -> i32 {
block0(v0: i32, v1: i32):
    v2 = ineg v1
    v3 = iadd v0, v2
    return v3
    ; check: v4 = isub v0, v1
    ; check: return v4
}

function %ineg_add(i32, i32) -> i32 {
block0(v0: i32, v1: i32):
    v2 = ineg v0
    v3 = iadd v2, v1
    return v3
    ; check: v4 = isub v1, v0
    ; check: return v4
}

function %sub_ineg(i32, i32) -> i32 {
block0(v0: i32, v1: i32):
    v2 = ineg v1
    v3 = isub v0, v2
    return v3
    ; check: v4 = iadd v0, v1
    ; check: return v4
}

function %sub_then_ineg(i32, i32) -> i32 {
block0(v0: i32, v1: i32):
    v2 = isub v0, v1
    v3 = ineg v2
    return v3
    ; check: v4 = isub v1, v0
    ; check: return v4
}

function %imul_ineg_cancel(i32, i32) -> i32 {
