test compile precise-output
target pulley64

function %simple(i64, i32) {
block0(v0: i64, v1: i32):
    v2 = load.i64 v0+16
    v3 = uextend.i64 v1
    v4 = iconst.i64 24
    v5 = isub v2, v4
    v6 = icmp ugt v3, v5
    trapnz v6, user1
    return
}

