function %twice(i64, i32, i32) {
block0(v0: i64, v1: i32, v2: i32):
    ;; load the bound & calculate what to check against
    v3 = load.i64 v0+16
    v4 = iconst.i64 24
    v5 = isub v3, v4

    ;; check v1
    v6 = uextend.i64 v1
    v7 = icmp ugt v6, v5
    trapnz v7, user1

    ;; check v2
    v8 = uextend.i64 v2
    v9 = icmp ugt v8, v5
    trapnz v9, user1

    return
}

