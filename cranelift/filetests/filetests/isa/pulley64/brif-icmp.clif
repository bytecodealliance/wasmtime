function %brif_icmp_eq(i32, i32) -> i32 {
block0(v0: i32, v1: i32):
  v2 = icmp eq v0, v1
  brif v2, block2, block1

block1:
  v3 = iconst.i32 1
  return v3

block2:
  v4 = iconst.i32 2
  return v4
}

; VCode:
; block0:
;   br_if_xeq32 x0, x1, label2; jump label1
; block1:
;   xconst8 x0, 1
;   ret
; block2:
;   xconst8 x0, 2
;   ret
;
; Disassembled:
; br_if_xeq32 x0, x1, 0xb    // target = 0xb
; xconst8 x0, 1
; ret
; xconst8 x0, 2
; ret

