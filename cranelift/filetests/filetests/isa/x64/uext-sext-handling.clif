test compile
target x86_64

; The x86_64 system_v call conv respects uext and sext
function u0:0(i8) system_v {
    sig0 = (i8 uext) system_v
    fn0 = u0:0 sig0

block0(v0: i8):
    call fn0(v0)
    return
}

