test compile
target aarch64

; The aapcs64 call conv ignores the uext and sext flags
function u0:0(i8) system_v {
    sig0 = (i8 uext) system_v
    fn0 = u0:0 sig0

block0(v0: i8):
    call fn0(v0)
    return
}

