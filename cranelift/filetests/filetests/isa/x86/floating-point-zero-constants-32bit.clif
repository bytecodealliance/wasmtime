; Check that floating-point and integer constants equal to zero are optimized correctly.
test binemit
target i686

function %foo() -> f32 fast {
block0:
  ; asm: xorps %xmm0, %xmm0
  [-,%xmm0]    v0 = f32const 0.0     ; bin: 0f 57 c0
  return v0
}

function %bar() -> f64 fast {
block0:
  ; asm: xorpd %xmm0, %xmm0
  [-,%xmm0]    v1 = f64const 0.0     ; bin: 66 0f 57 c0
  return v1
}
