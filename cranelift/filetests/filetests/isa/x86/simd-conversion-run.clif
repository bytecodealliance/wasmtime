test run
set enable_simd
target x86_64

function %fcvt_from_sint() -> b1 {
block0:
    v0 = vconst.i32x4 [-1 0 1 123456789]
    v1 = fcvt_from_sint.f32x4 v0

    v2 = vconst.f32x4 [-0x1.0 0.0 0x1.0 0x75bcd18.0] ; 123456789 rounds to 123456792.0, an error of 3
    v3 = fcmp eq v1, v2
    v4 = vall_true v3
    return v4
}
; run
