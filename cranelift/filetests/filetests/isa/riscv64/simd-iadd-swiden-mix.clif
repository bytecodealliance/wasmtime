test compile precise-output
set unwind_info=false
target riscv64 has_v

function %iadd_swiden_high_low_i32x4(i32x4, i32x4) -> i64x2 {
block0(v0: i32x4, v1: i32x4):
    v2 = swiden_high v0
    v3 = swiden_low v1
    v4 = iadd v2, v3
    return v4
}

