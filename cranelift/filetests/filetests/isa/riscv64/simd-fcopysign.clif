test compile precise-output
set unwind_info=false
target riscv64 has_v


function %fcopysign_f32x4(f32x4, f32x4) -> f32x4 {
block0(v0: f32x4, v1: f32x4):
    v2 = fcopysign v0, v1
    return v2
}

