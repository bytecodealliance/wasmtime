function %trap() {
block0:
  trap user0
}

; VCode:
; block0:
;   .word 0x0000 # trap=user0
;
; Disassembled:
; block0: ; offset 0x0
;   .byte 0x00, 0x00 ; trap: user0

function %trapz(i64) {
block0(v0: i64):
  v1 = iconst.i64 42
  v2 = icmp eq v0, v1
  trapz v2, user0
  return
}

; VCode:
