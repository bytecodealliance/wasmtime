; Stack slot references
function %stack() {
    ss10 = spill_slot 8
    ss2 = explicit_slot 4
    ss3 = incoming_arg 4, offset 8
    ss4 = outgoing_arg 4
    ss5 = emergency_slot 4

block0:
    v1 = stack_load.i32 ss10
    v2 = stack_load.i32 ss10+4
    stack_store v1, ss10+2
    stack_store v2, ss2
}
