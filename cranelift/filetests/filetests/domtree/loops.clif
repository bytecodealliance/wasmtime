function %test(i32) {
    block0(v0: i32):
        brz v0, block1  ; dominates: block1 block3 block4 block5
        jump block2     ; dominates: block2
    block1:
        jump block3
    block2:
        brz v0, block4
        jump block5
    block3:
        jump block4
    block4:
        brz v0, block3
        jump block5
    block5:
        brz v0, block4
        jump block6     ; dominates: block6
    block6:
        return
}
