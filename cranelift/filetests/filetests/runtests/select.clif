function %select_overflow_i8(i8) -> i8 {
block0(v0: i8):
    v1 = iconst.i8 255
    v2 = iadd v0, v1
    v3 = iconst.i8 1
    v4 = iconst.i8 0
    v5 = select v2, v3, v4
    return v5
}

; run: %select_overflow_i8(0) == 1
; run: %select_overflow_i8(2) == 1
; run: %select_overflow_i8(1) == 0
; run: %select_overflow_i8(98) == 1

