    v4 = bconst.b1 false
    v5 = select v2, v3, v4
    return v5
}
; run: %select_gt_f64(0x42.42, 0.0) == true
; run: %select_gt_f64(0.0, 0.0) == false
; run: %select_gt_f64(0x0.0, 0x42.42) == false
; run: %select_gt_f64(NaN, 0x42.42) == false

