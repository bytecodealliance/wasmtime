function %bor_splat_f32x4(f32x4, i32) -> f32x4 {
block0(v0: f32x4, v1: i32):
    v2 = bitcast.f32 v1
    v3 = splat.f32x4 v2
    v4 = bor v0, v3
    return v4
}
