; Icmp Imm Tests
function %icmp_imm_eq_i128() -> b1 {
block0:
    v11 = iconst.i64 0x0
    v12 = iconst.i64 0x0
    v1 = iconcat v11, v12
    v10 = icmp_imm.i128 eq v1, 0x0
    return v10
}

; run: %icmp_imm_eq_i128() == true

function %icmp_imm_ne_i128() -> b1 {
block0:
    v11 = iconst.i64 0x0
    v12 = iconst.i64 0x0
    v1 = iconcat v11, v12
    v10 = icmp_imm.i128 ne v1, 0x1
    return v10
}

; run: %icmp_imm_ne_i128() == true
