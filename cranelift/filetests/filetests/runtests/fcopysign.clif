function %fcopysign_f32(f32, f32) -> f32 {
block0(v0: f32, v1: f32):
    v2 = fcopysign v0, v1
    return v2
}
; run: %fcopysign_f32(0x9.0, 0x9.0) == 0x9.0
; run: %fcopysign_f32(-0x9.0, 0x9.0) == 0x9.0
